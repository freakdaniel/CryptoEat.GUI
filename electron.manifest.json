{
  "executable": "CGUI",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "CryptoEat",
  "author": "CryptoEat Team",
  "singleInstance": true,
  "environment": "Development",
  "build": {
    "appId": "com.cryptoeat.app",
    "productName": "CryptoEat",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!**/node_modules/**/*",
      "!**/*.pdb"
    ],
    "extraResources": [
      {
        "from": "ClientApp/node_modules",
        "to": "app/node_modules",
        "filter": ["**/*"]
      }
    ]
  },
  "aspCoreBackendPort": 8001
}
